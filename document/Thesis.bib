@online{heLinkGPTTeachingLarge2024,
  title = {{{LinkGPT}}: {{Teaching Large Language Models To Predict Missing Links}}},
  shorttitle = {{{LinkGPT}}},
  author = {He, Zhongmou and Zhu, Jing and Qian, Shengyi and Chai, Joyce and Koutra, Danai},
  date = {2024-06-07},
