@online{liCondensedTransitionGraph2024,
  title = {A {{Condensed Transition Graph Framework}} for {{Zero-shot Link Prediction}} with {{Large Language Models}}},
  author = {Li, Mingchen and Ling, Chen and Zhang, Rui and Zhao, Liang},
  date = {2024-11-26},
  eprint = {2402.10779},
  eprinttype = {arXiv},
